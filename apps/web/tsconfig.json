{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],

    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",

    "noEmit": true,
    "types": ["vite/client"],
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/*"]
}